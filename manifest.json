{
  "name": "SAP AW+",
  "description": "SAP Advanced Workflow Plus",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": ["scripting", "activeTab"],
  "action": { 
    "default_popup": "main_menu.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.webcomserver.com/*"],
      "css": ["css/content.css"],
      "js": ["js/activate.js"]
    }
  ],
  "icons": { 
    "16": "media/ccg_logo_light_xs.png",
    "32": "media/ccg_logo_light_xs.png",
    "48": "media/ccg_logo_light_xs.png",
    "128":"media/ccg_logo_light_xs.png"
  },
  "options_page": ""
}